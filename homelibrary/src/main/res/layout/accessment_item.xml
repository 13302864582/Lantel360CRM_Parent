<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="@dimen/accept_sms_paddingHori"
    android:layout_marginRight="@dimen/accept_sms_paddingHori"
    android:layout_marginTop="@dimen/accept_sms_paddingVerti">
<ImageView
    android:id="@+id/circle_img"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintHorizontal_bias="0.165"
    android:layout_marginTop="@dimen/accept_sms_paddingVerti"
    android:layout_width="@dimen/circle_img_access"
    android:layout_height="@dimen/circle_img_access" />
</androidx.constraintlayout.widget.ConstraintLayout>